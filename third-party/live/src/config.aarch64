CROSS_HOME = /usr/local/gcc-arm-9.2-2019.12-x86_64-aarch64-none-linux-gnu/aarch64-none-linux-gnu
CROSS_COMPILE?=		aarch64-none-linux-gnu-
COMPILE_OPTS =		$(INCLUDES) -I$(CROSS_HOME)/include -I$(CROSS_HOME)/libc/usr/include -I$(HOME_PATH)/openssl/include -I. -O2 -DSOCKLEN_T=socklen_t -DNO_SSTREAM=1 -D_LARGEFILE_SOURCE=1 -D_FILE_OFFSET_BITS=64
C =			c
C_COMPILER =		$(CROSS_COMPILE)gcc
C_FLAGS =		$(COMPILE_OPTS) -fPIC
CPP =			cpp
CPLUSPLUS_COMPILER =	$(CROSS_COMPILE)g++
CPLUSPLUS_FLAGS =	$(COMPILE_OPTS) -Wall -DBSD=1 -fPIC
OBJ =			o
LINK =			$(CROSS_COMPILE)g++ -o
LINK_OPTS =
CONSOLE_LINK_OPTS =	$(LINK_OPTS)
LIBRARY_LINK =         $(CROSS_COMPILE)ar cr
LIBRARY_LINK_OPTS =	$(LINK_OPTS)
LIB_SUFFIX =			a
LIBS_FOR_CONSOLE_APPLICATION = -L${HOME_PATH}/openssl/lib -lssl -lcrypto
LIBS_FOR_GUI_APPLICATION =
EXE =
